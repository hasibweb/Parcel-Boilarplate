{
  "name": "parcel-test",
  "version": "1.0.0",
  "description": "My awesome app",
  "repository": {},
  "dependencies": {},
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.14",
    "sass": "^1.26.10",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "dev": "npm run clean && parcel src/index.html --out-dir development -p 3000",
    "build": "parcel build src/*.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache"
  },
  "author": "Hasib",
  "license": "ISC",
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".js.map": "js",
      "img": [
        ".jpg",
        ".png",
        ".svg"
      ],
      ".css": "css",
      ".css.map": "css"
    }
  }
}
